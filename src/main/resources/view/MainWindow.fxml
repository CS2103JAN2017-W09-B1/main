<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@DarkTheme.css" />
      <URL value="@Extensions.css" />
   </stylesheets>
   <children>
      <SplitPane id="splitPane" fx:id="splitPane" dividerPositions="0.4" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <items>
            <VBox fx:id="nonFloatingTaskList" minWidth="340" prefWidth="340" SplitPane.resizableWithParent="false">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
               <children>
                  <AnchorPane fx:id="nonFloatingTaskListPanelPlaceholder" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox fx:id="completedTaskList" minWidth="330" prefWidth="330" SplitPane.resizableWithParent="false">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
               <children>
                  <AnchorPane fx:id="completedTaskListPanelPlaceholder" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <!--
            <AnchorPane fx:id="browserPlaceholder" prefWidth="340">
                <padding>
                    <Insets bottom="10" left="10" right="10" top="10" />
                </padding>
            </AnchorPane>
            -->
         </items>
      </SplitPane>
      <SplitPane id="splitPane" fx:id="splitPane1" dividerPositions="0.4" layoutX="10.0" layoutY="155.0">
         <items>
            <VBox fx:id="floatingTaskList" minWidth="340" prefWidth="340" SplitPane.resizableWithParent="false">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <AnchorPane fx:id="floatingTaskListPanelPlaceholder" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox fx:id="floatingTaskList1" SplitPane.resizableWithParent="false">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                   <AnchorPane fx:id="resultDisplayPlaceholder" minHeight="370.0" minWidth="300.0" VBox.vgrow="NEVER" />
                  <AnchorPane fx:id="commandBoxPlaceholder" VBox.vgrow="NEVER">
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </AnchorPane>
               </children>
            </VBox>
         </items>
      </SplitPane>
      <AnchorPane fx:id="statusbarPlaceholder" VBox.vgrow="NEVER" />
   </children>
</VBox>
