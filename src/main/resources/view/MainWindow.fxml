<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<VBox minHeight="500.0" minWidth="500.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@DarkTheme.css" />
      <URL value="@Extensions.css" />
   </stylesheets>
   <children>
       <AnchorPane fx:id="resultDisplayPlaceholder" minHeight="160.0" minWidth="250.0" style="-fx-background-color: transparent; -fx-border-color: black; -fx-border-width: 4;" VBox.vgrow="ALWAYS">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
         </VBox.margin>
       </AnchorPane>
      <AnchorPane fx:id="commandBoxPlaceholder" minHeight="50.0" minWidth="250.0" VBox.vgrow="NEVER">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </AnchorPane>
      <SplitPane id="splitPane" fx:id="splitPane" dividerPositions="0.5, 0.5" layoutX="10.0" layoutY="155.0" minHeight="250.0" minWidth="500.0" styleClass="split-pane-divider">
         <items>
            <VBox fx:id="nonFloatingTaskList" minHeight="250.0" minWidth="250.0" style="-fx-background-color: #ff6666;">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
               <children>
                  <AnchorPane fx:id="nonFloatingTaskListPanelPlaceholder" minWidth="250.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            <VBox fx:id="floatingTaskList">
               <children>
                  <AnchorPane fx:id="floatingTaskListPanelPlaceholder" minWidth="250.0" />
               </children>
            </VBox>
            <VBox fx:id="completedTaskList" minHeight="250.0" minWidth="250.0" style="-fx-background-color: #ffcc66;">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
               <children>
                  <AnchorPane fx:id="completedTaskListPanelPlaceholder" minWidth="250.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </items>
      </SplitPane>
      <TextFlow maxWidth="90.0" minHeight="40.0" minWidth="90.0" style="-fx-background-color: #333333;" translateX="-10.0" translateY="-10.0">
         <children>
            <Text fill="#33cc66" strokeType="OUTSIDE" strokeWidth="0.0" text="Floating" translateX="10.0" translateY="10.0">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Text>
         </children>
      </TextFlow>
      <TextFlow maxWidth="110.0" minHeight="40.0" style="-fx-background-color: #333333;">
         <children>
            <Text fill="#6699cc" strokeType="OUTSIDE" strokeWidth="0.0" text="Command">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Text>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </TextFlow>
      <TextFlow maxWidth="90.0" minHeight="40.0" style="-fx-background-color: #333333;">
         <children>
            <Text fill="#ff6666" strokeType="OUTSIDE" strokeWidth="0.0" text="Ongoing" translateX="10.0" translateY="10.0">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Text>
         </children>
      </TextFlow>
      <TextFlow maxWidth="110.0" minHeight="40.0" style="-fx-background-color: #333333;">
         <children>
            <Text fill="#ffcc66" strokeType="OUTSIDE" strokeWidth="0.0" text="Completed" translateX="10.0" translateY="10.0">
               <font>
                  <Font name="Arial Bold" size="18.0" />
               </font>
            </Text>
         </children>
      </TextFlow>
   </children>
</VBox>
